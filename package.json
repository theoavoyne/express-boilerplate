{
  "name": "hbs-boilerplate",
  "version": "1.0.0",
  "description": "HBS Boilerplate with Webpack",
  "main": "server.js",
  "author": "Theo Avoyne",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "build:dev": "webpack --env development",
    "build:prod": "webpack --env production",
    "dev-server": "export NODE_ENV=development || SET \"NODE_ENV=development\" && nodemon server.js nodemon -e js,hbs",
    "heroku-postbuild": "yarn run build:prod"
  },
  "engines": {
    "node": "11.10.0"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "bootstrap": "^4.3.1",
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "hbs": "^4.0.3",
    "jquery": "^3.3.1",
    "morgan": "^1.9.1",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "popper.js": "^1.14.7",
    "sass-loader": "^7.1.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.10",
    "webpack-dev-middleware": "^3.6.1"
  }
}
